def is_perfect_number(number):
# write your code here...
if(number<1):
return False
result=0
for i in range (1,number):
if(number%i==0):
result=result+i
if(result==number):
return True
else:
return False
roll_number = int(input())
if is_perfect_number(roll_number):
 print("Perfect number")
else:
 print("Not a perfect number")
